{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run reactproj-reactmodule",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/reactmodule/index.html"
      ],
      "cwd": "${workspaceFolder}/reactmodule/build",
